     <%= simple_form_for [@cocktail, @dose] do |f| %>
        <%= f.error_notification %>
        <%= f.input :description %>
        <%= f.association :ingredient %>
        <%= f.association :cocktail %>
        <%= f.button :submit, class: "btn btn-primary" %>
      <% end %>

      <%= link_to "back", cocktail_path(@cocktail) %>
